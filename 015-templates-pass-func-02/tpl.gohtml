<body>
<ul>
     {{range $index, $value := .Wisdom}}
    <li>{{ft $value.Name}}-{{uc $value.Motto}}</li>
    {{end}}
</ul>
<ul>
     {{range $index, $value := .Transport}}
    <li>{{uc $value.Manufacturer}}-{{$value.Doors}}</li>
    {{end}}
</ul>
</body>
</html>